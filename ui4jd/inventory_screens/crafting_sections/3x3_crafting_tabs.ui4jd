            { "11@ui_4jd_textures.cell_image_1_5": { "size": [ 27, 27 ] } },
            { "12@ui_4jd_textures.cell_image_1_5": { "size": [ 27, 27 ] } }
          ]
        }
      },
      {
        "category_label@common_4jd.global_label": {
          "color": "$title_text_color",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [ 0, 16.5 ],
          "layer": 5,
          "text": "$catregory_text"
        }
      }
    ]
  }
}