    "orientation": "horizontal",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ 503, 190 ],
    "offset": [ 9, 11 ],
    "controls": [
      {
        "left_panel@ui_4jd_textures.panel_recess": {
          "size": [ "fill", "100%" ],
